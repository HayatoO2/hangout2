<h2 class="mt-3">登録情報</h2>
<div class="wrapper">
    
    
      <div class="box2">
    
  
    <div class="show">
    <p class="my-5"> <%= "ニックネーム   :#{current_user.nickname}" %> </p>
    <p class="my-5"> <%= "メールアドレス  :#{current_user.email}" %> </p>
    <%= link_to "編集する", edit_user_path(current_user.id), class: "btn btn-dark my-5", id: "edit-btn" %>
</div>
</div>
      
      <div class= "box2">
    <div class="show">
      <h2 class="mb-3">所属スペース</h2>
      <div>
    <% @shares.each do |share| %> 
          <p><%= link_to "#{share.name}", new_share_present_path(share.id)  %></p>
          <% end %>
    </div>
    
      </div>

      
      
      

</div>
